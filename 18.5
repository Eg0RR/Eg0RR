#include <iostream>
#include <ctime>
using namespace std;

int main()
{
	srand((unsigned)time(NULL));
	setlocale(LC_ALL, "Russian");
	int length;
	cin >> length;
	int* array = new int[length];
	//array[length] = { 7, 0, 1, 2, 3 };
	array[0] = 7;
	array[1] = 0;
	array[2] = 1;
	array[3] = 8;
	array[4] = 9;
	int x = array[0];
	int index = 0;
	for (int i = 0; i < length; i++)
	{
	//	array[i] = rand() % 50 + 51;
		cout << " array[" << i << "]  = " << array[i] << endl;
	}
	for (int i = 0; i < length; i++)
	{
		if (x < array[i])
		{
			index = i;
			break;
		}
		
	}
	for (int i = 1; i < index; i++)
		array[i - 1] = array[i];
	array[index - 1] = x;
	for (int i = 0; i < length; i++)
	{
		//	array[i] = rand() % 50 + 51;
		cout << " array[" << i << "]  = " << array[i] << endl;
	}
}
